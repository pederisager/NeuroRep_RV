@article{ ISI:000263232800003,
Author = {Northoff, Georg and Schneider, Felix and Rotte, Michael and Matthiae,
   Christian and Tempelmann, Claus and Wiebking, Christina and Bermpohl,
   Felix and Heinzel, Alexander and Danos, Peter and Heinze, Hans-Jochen
   and Bogerts, Bernhard and Walter, Martin and Panksepp, Jaak},
Title = {{Differential Parametric Modulation of Self-Relatedness and Emotions in
   Different Brain Regions}},
Journal = {{HUMAN BRAIN MAPPING}},
Year = {{2009}},
Volume = {{30}},
Number = {{2}},
Pages = {{369-382}},
Month = {{FEB}},
Abstract = {{Our sense of self is strongly colored by emotions although at the same
   time we are well able to distinguish affect and self. Using functional
   magnetic resonance imaging, we here tested for the differential effects
   of self-relatedness and emotion dimensions (valence, intensity) on
   parametric modulation of neural activity during perception of emotional
   stimuli. We observed opposite parametric modulation of self-relatedness
   and emotion dimensions in the dorsomedial prefrontal cortex and the
   ventral striatum/nucleus accumbens, whereas neural activity in
   subcortical regions (tectum, right amygdala, hypothalamus) was modulated
   by self-relatedness and emotion dimensions in the same direction. In
   sum, our results demonstrate that self-relatedness is closely linked to
   emotion dimensions of valence and intensity in many lower subcortical
   brain regions involved in basic emotional systems and, at the same time,
   distinct from them in higher cortical regions that mediate cognitive
   processes necessary for becoming aware of one's self, for example
   self-consciousness. Hum Brain Mapp 30:369-382, 2009. (C) 2007
   Wiley-Liss, Inc.}},
