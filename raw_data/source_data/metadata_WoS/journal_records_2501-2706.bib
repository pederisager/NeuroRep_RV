@article{ ISI:000266458900024,
Author = {Poulos, Andrew M. and Nobuta, Hiroko and Thompson, Richard F.},
Title = {{Disruption of Cerebellar Cortical Inhibition in the Absence of Learning
   Promotes Sensory-Evoked Eyeblink Responses}},
Journal = {{BEHAVIORAL NEUROSCIENCE}},
Year = {{2009}},
Volume = {{123}},
Number = {{3}},
Pages = {{694-700}},
Month = {{JUN}},
Abstract = {{Theories of cerebellar learning propose that alterations in synaptic
   plasticity resulting in decreases in cerebellar cortical inhibition and
   increases in sensory activation of interpositus nuclei underlie the
   development of adaptively timed conditioned motor responses. The authors
   found that with concurrent pharmacological disconnection of the
   cerebellar cortex and intense sensory stimulation in the untrained
   rabbit, eyeblink responses were generated. Neither sensory stimulation
   nor disconnection alone generated significant eyeblink responses. These
   results are consistent with dual plasticity models of cerebellar
   learning and strongly support the general hypothesis that conditioned
   responses are the result of strengthening of preexisting connections in
   the nervous system.}},
