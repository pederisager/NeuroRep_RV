@article{ ISI:000264565800012,
Author = {Gilby, Krista L. and Sydserff, Simon and Patey, Andrea M. and Thorne,
   Victoria and St-Onge, Veronique and Jans, Jennifer and McIntyre, Dan C.},
Title = {{Postnatal Epigenetic Influences on Seizure Susceptibility in
   Seizure-Prone Versus Seizure-Resistant Rat Strains}},
Journal = {{BEHAVIORAL NEUROSCIENCE}},
Year = {{2009}},
Volume = {{123}},
Number = {{2}},
Pages = {{337-346}},
Month = {{APR}},
Abstract = {{The creation of seizure-prone (Fast) and seizure-resistant (Slow) rat
   strains via selective breeding implies genetic control of relative
   seizure vulnerability, yet ample data also advocates an environmental
   contribution. To investigate potential environmental underpinnings to
   the differential seizure sensitivities in these strains, the authors
   compared amygdala kindling profiles in adult male Fast and Slow rats
   raised by (a) their own mother, (b) a foster mother from the same
   strain, or (c) a foster mother from the opposing strain. Ultimately,
   strain-specific kindling profiles were not normalized by
   cross-fostering. Instead, both strains became more seizure-prone
   regardless of maternal affiliation (i.e., cross-fostered groups from
   both strains kindled faster than uncrossed controls). Interhemispheric
   seizure spread was also facilitated in cross-fostered Slow rat groups
   and was associated with increased commissural cross-sectional areas,
   giving them a Fast-like profile. It is important to note, however, that
   all Fast groups remained significantly more seizure-prone than Slow
   groups, suggesting that although tire postnatal environment strongly
   influenced seizure disposition in both strains, it did not wholly
   account for their relative dispositions. Investigation into mechanisms
   fundamental to cross-fostering-induced seizure facilitation should help
   prevent postnatal worsening of pathology in already seizure-prone
   individuals.}},
