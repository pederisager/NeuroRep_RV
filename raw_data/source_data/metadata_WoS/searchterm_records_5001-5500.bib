@article{ ISI:000267796100092,
Author = {Zaki, Jamil and Weber, Jochen and Bolger, Niall and Ochsner, Kevin},
Title = {{The neural bases of empathic accuracy}},
Journal = {{PROCEEDINGS OF THE NATIONAL ACADEMY OF SCIENCES OF THE UNITED STATES OF
   AMERICA}},
Year = {{2009}},
Volume = {{106}},
Number = {{27}},
Pages = {{11382-11387}},
Month = {{JUL 7}},
Abstract = {{Theories of empathy suggest that an accurate understanding of another's
   emotions should depend on affective, motor, and/or higher cognitive
   brain regions, but until recently no experimental method has been
   available to directly test these possibilities. Here, we present a
   functional imaging paradigm that allowed us to address this issue. We
   found that empathically accurate, as compared with inaccurate, judgments
   depended on (i) structures within the human mirror neuron system thought
   to be involved in shared sensorimotor representations, and (ii) regions
   implicated in mental state attribution, the superior temporal sulcus and
   medial prefrontal cortex. These data demostrate that activity in these 2
