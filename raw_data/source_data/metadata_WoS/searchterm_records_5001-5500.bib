@article{ ISI:000268068600003,
Author = {Sebastian, Catherine and Blakemore, Sarah-Jayne and Charman, Tony},
Title = {{Reactions to Ostracism in Adolescents with Autism Spectrum Conditions}},
Journal = {{JOURNAL OF AUTISM AND DEVELOPMENTAL DISORDERS}},
Year = {{2009}},
Volume = {{39}},
Number = {{8}},
Pages = {{1122-1130}},
Month = {{AUG}},
Abstract = {{Little is known about how adolescents with autism spectrum conditions
   (ASC) experience the initial impact of ostracism. This study
   investigated whether a mild, short-term episode of experimentally
   induced ostracism (Cyberball) would affect self-reported anxiety, mood,
   and the extent to which four social needs (self-esteem, belonging,
   control and meaningful existence) were threatened in adolescents with
   ASC and matched controls. Anxiety and the four needs were negatively
   affected by ostracism in both groups. However, ostracism did not
   modulate mood in the ASC group, and a number of possible interpretations
   of this group difference are discussed. In general, the results of this
   study suggest that normative models of ostracism are applicable to ASC.}},
