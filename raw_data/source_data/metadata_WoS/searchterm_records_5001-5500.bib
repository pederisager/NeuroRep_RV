@article{ ISI:000269360700014,
Author = {Platek, Steven M. and Krill, Austen L.},
Title = {{Self-face resemblance attenuates other-race face effect in the amygdala}},
Journal = {{BRAIN RESEARCH}},
Year = {{2009}},
Volume = {{1284}},
Pages = {{156-160}},
Month = {{AUG 11}},
Abstract = {{People respond favorably toward self-resembling faces. We investigated
   the pattern of responding in the amygdala of Caucasian participants to
   self-face resemblance expressed in same and other-race (African descent)
   faces. The amygdala response was 1) non-linear to faces as a function of
   self-facial resemblance and 2) attenuated to other-race self-resembling
   faces when regressed with implicit racial attitudes. These findings
   demonstrate that interactions of important facial social judgements are
   processed combinatorially in the amygdala. (c) 2009 Elsevier B.V. All
   rights reserved.}},
