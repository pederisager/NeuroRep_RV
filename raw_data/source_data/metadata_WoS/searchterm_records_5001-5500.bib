@article{ ISI:000270741300003,
Author = {Tesink, Cathelijne M. J. Y. and Petersson, Karl Magnus and van Berkum,
   Jos J. A. and van den Brink, Danielle and Buitelaar, Jan K. and Hagoort,
   Peter},
Title = {{Unification of Speaker and Meaning in Language Comprehension: An fMRI
   Study}},
Journal = {{JOURNAL OF COGNITIVE NEUROSCIENCE}},
Year = {{2009}},
Volume = {{21}},
Number = {{11}},
Pages = {{2085-2099}},
Month = {{NOV}},
Abstract = {{When interpreting a message, a listener takes into account several
   sources of linguistic and extralinguistic information. Here we focused
   on one particular form of extralinguistic information, certain speaker
   characteristics as conveyed by the voice. Using functional magnetic
   resonance imaging, we examined the neural structures involved in the
   unification of sentence meaning and voice-based inferences about the
   speaker's age, sex, or social background. We found enhanced activation
   in the inferior frontal gyrus bilaterally ( BA 45/47) during listening
   to sentences whose meaning was incongruent with inferred speaker
   characteristics. Furthermore, our results showed an overlap in brain
