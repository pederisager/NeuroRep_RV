@incollection{ ISI:000277791700008,
Author = {Bennett, Craig M. and Miller, Michael B.},
Editor = {{Kingstone, A and Miller, MB}},
Title = {{How reliable are the results from functional magnetic resonance imaging?}},
Booktitle = {{YEAR IN COGNITIVE NEUROSCIENCE 2010}},
Series = {{Annals of the New York Academy of Sciences}},
Year = {{2010}},
Volume = {{1191}},
Pages = {{133-155}},
Abstract = {{Functional magnetic resonance imaging (fMRI) is one of the most
   important methods for in vim in vestigation of cognitive processes in
   the human brain. Within the last two decades, an explosion of research
   has emerged using fMRI, revealing the underpinnings of everything from
   motor and sensory processes to the foundations of social cognition.
   While these results have revealed the potential of neuroimaging,
   important questions regarding title reliability of these results remain
   unanswered. In this paper, we take a close look at what is currently
   known about the reliability of fMRI findings. First, we examine the many
   factors that influence the quality of acquired fMRI data. We also
   conduct a review of the existing literature to determine if some measure
   of agreement has emerged regarding the reliability of fMRI. Finally, we
   provide commentary on ways to improve fMRI reliability and what
   questions remain unanswered. Reliability is the foundation on which
   scientific investigation is based. How reliable are the results from
   fMRI?}},
