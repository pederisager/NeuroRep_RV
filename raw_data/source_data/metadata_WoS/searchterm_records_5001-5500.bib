@article{ ISI:000276906800004,
Author = {Yu, Caiyun and Tu, Shen and Wang, Ting and Qiu, Jiang},
Title = {{The neural basis of self-evaluation processing in social judgment}},
Journal = {{NEUROREPORT}},
Year = {{2010}},
Volume = {{21}},
Number = {{7}},
Pages = {{497-501}},
Month = {{MAY 12}},
Abstract = {{Event-related potentials were recorded to explore the
   electrophysiological correlates of self-evaluation processing when
   participants performed intelligence social judgment tasks (judging who
   is more intelligent), containing self-judgment tasks (ST) and other
   judgment tasks (OT). Results showed that ST elicited a greater
   positivity (P2) than OT in 130-250 ms, which was related to initial
   self-related stimuli identification. Subsequently, ST elicited a greater
   positivity (P3) and negativity (LNC) than OT in 300-400 and 600-800 ms.
   The generator of P3 was localized in the right superior temporal gyrus,
   which was related to elaborate intelligence self-evaluation. The
   generator of LNC was localized in the posterior cingulate gyrus, which
   was involved in making and validating a judgment in working memory.
   NeuroReport 21:497-501 (C) 2010 Wolters Kluwer Health vertical bar
