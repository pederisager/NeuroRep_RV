@article{ ISI:000285285600008,
Author = {Klein, Stanley B. and Lax, Moshe L. and Gangi, Cynthia E.},
Title = {{A CALL FOR AN INCLUSIVE APPROACH TO THE SOCIAL COGNITIVE NEUROSCIENCES}},
Journal = {{SOCIAL COGNITION}},
Year = {{2010}},
Volume = {{28}},
Number = {{6}},
Pages = {{748-756}},
Month = {{DEC}},
Abstract = {{As part of a general trend toward interdisciplinary approaches, the
   social cognitive neurosciences was founded on the premise that research
   into social issues that take the brain seriously have much to contribute
   to our understanding of social thought and behavior. However, despite
   this goal, reliance on a specific set of techniques-neuroimaging-has
   come to dominate the emerging field. In this paper we argue that (a) an
   inclusive social cognitive neuroscience, one that takes seriously all
   three constituents of the field's name, is likely to provide greater
   insight into the nature of human social behavior, and (b) within the
   domain of neuroscience, no one method-be it brain scanning, patient
   populations, or case studies of patients suffering neurological
   impairment-holds the key to understanding the neural underpinnings of
   social behavior.}},
