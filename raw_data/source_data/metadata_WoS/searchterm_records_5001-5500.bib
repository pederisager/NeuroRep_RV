@article{ ISI:000272288800003,
Author = {Gunther, Max L. and Beach, Steven R. H. and Yanasak, Nathan E. and
   Miller, L. Stephen},
Title = {{Deciphering spousal intentions: An fMRI study of couple communication}},
Journal = {{JOURNAL OF SOCIAL AND PERSONAL RELATIONSHIPS}},
Year = {{2009}},
Volume = {{26}},
Number = {{4}},
Pages = {{388-410}},
Month = {{JUN}},
Abstract = {{Several relational theories suggest that advice, particularly advice in
   areas important to the self, may be cognitively processed differently
   than other types of support (e. g., nondirective support) or low
   importance advice. Little is known, however, about the neurocognitive
   substrates of such complex social behaviors. We hypothesized that the
   Superior Temporal Sulcus (STS) and the Medial Prefrontal Cortex (MPFC),
   cortical regions previously linked to Theory of Mind (ToM), would be
   more active for high than low importance advice. Results indicated that
   high importance advice was associated with greater activation in the
   left MPFC and bilaterally in the STS. Similar results were obtained when
   compared to positive comments. These findings indicate that when given
   advice individuals may be attempting to infer motivation.}},
