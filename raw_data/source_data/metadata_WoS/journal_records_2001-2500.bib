@article{ ISI:000278466000014,
Author = {King, Bradley J. and Furlong, Teri M. and McNally, Gavan P.},
Title = {{Cocaine and Amphetamine Related Transcript (CART) Inhibits Context
   Induced Reinstatement of Reward Seeking}},
Journal = {{BEHAVIORAL NEUROSCIENCE}},
Year = {{2010}},
Volume = {{124}},
Number = {{3}},
Pages = {{423-427}},
Month = {{JUN}},
Abstract = {{We studied the role of cocaine and amphetamine related transcript (CART)
   in regulating context induced reinstatement (renewal) of reward seeking.
   Rats were trained to respond for alcoholic beer in context A before
   extinction in context B. Rats were tested for responding in context A
   (ABA) and context B (ABB). Intracerebroventricular (ICV) infusions of
   the active fragment CART55-102 but not the inactive fragment CARTI-27
   before test prevented ABA renewal of extinguished responding. ICV
   CART55-102 had no effect on responding in the extinction context (ABB).
   ICV CART55-102 also altered the profile of behavioral responses observed
   on test in the training (ABA) but not extinction (ABB) context. These
   results identify a novel role for CART in preventing reinstatement of
   reward seeking.}},
