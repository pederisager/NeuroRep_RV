@article{ ISI:000272738100003,
Author = {Lee, Grace and Gammie, Stephen C.},
Title = {{GABA(A) Receptor Signaling in the Lateral Septum Regulates Maternal
   Aggression in Mice}},
Journal = {{BEHAVIORAL NEUROSCIENCE}},
Year = {{2009}},
Volume = {{123}},
Number = {{6}},
Pages = {{1169-1177}},
Month = {{DEC}},
Abstract = {{Maternal aggression (maternal defense) is a fierce aggression produced
   by lactating females toward intruders that plays an important role in
   protection of Vulnerable offspring Enhancement of GAB(A), receptor
   signaling by benzodiazepines increases maternal aggression, and we
   recently found indirect evidence that lateral septum (I-S) could be a
   key site where benzodiazepines elevate aggression In this study. we
   directly tested the hypothesis that activation of GABA(A) receptors in
   LS would promote maternal aggression while inhibition of this receptor
   would decrease aggression Site-directed injections to LS were made using
   the GABA(A) receptor antagonist. bicuculline (3-30 ng), or the GABA(A)
   receptor agonists, chlordiazepoxide, a benzodiazepine (2.5-5 mu g), and
   muscimol (0.05-5 ng) Maternal aggression and other behavioral measures
   were then evaluated in lactating mice Neither GABA(A) receptor agonist
   elevated aggression. which could reflect a ceiling effect However. 7 ng
   of the GABA(A) receptor antagonist. bicuculline, in LS significantly
   decreased maternal aggression without altering other maternal behaviors
   or light-dark box performance. suggesting some GABA(A) receptor
