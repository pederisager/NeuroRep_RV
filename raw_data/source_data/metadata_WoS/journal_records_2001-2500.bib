@article{ ISI:000272738100010,
Author = {Marco, Asaf and Schroeder, Mariana and Weller, Aron},
Title = {{Microstructural Pattern of Palatable Food Intake From Weaning to
   Adulthood in Male and Female OLETF Rats}},
Journal = {{BEHAVIORAL NEUROSCIENCE}},
