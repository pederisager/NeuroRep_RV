@article{ ISI:000282071900004,
Author = {Duque, Juan F. Dominguez and Turner, Robert and Lewis, E. Douglas and
   Egan, Gary},
Title = {{Neuroanthropology: a humanistic science for the study of the
   culture-brain nexus}},
Journal = {{SOCIAL COGNITIVE AND AFFECTIVE NEUROSCIENCE}},
Year = {{2010}},
Volume = {{5}},
Number = {{2-3}},
Pages = {{138-147}},
Month = {{JUN}},
Abstract = {{In this article, we argue that a combined anthropology/neuroscience
   field of enquiry can make a significant and distinctive contribution to
   the study of the relationship between culture and the brain. This field,
   which can appropriately be termed as neuroanthropology, is conceived of
   as being complementary to and mutually informative with social and
   cultural neuroscience. We start by providing an introduction to the
   culture concept in anthropology. We then present a detailed
   characterization of neuroanthropology and its methods and how they
   relate to the anthropological understanding of culture. The field is
   described as a humanistic science, that is, a field of enquiry founded
   on the perceived epistemological and methodological interdependence of
   science and the humanities. We also provide examples that illustrate the
   proposed methodological model for neuroanthropology. We conclude with a
   discussion about specific contributions the field can make to the study
   of the culture-brain nexus.}},
