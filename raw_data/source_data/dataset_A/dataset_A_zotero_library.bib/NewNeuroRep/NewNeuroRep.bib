	keywords = {Computer Science - Social and Information Networks, Physics - Physics and Society, Quantitative Biology - Neurons and Cognition},
	pages = {711--731},
	annote = {Comment: 11 pages, 4 figures, 2 C codes, to appear in Advances in Complex Systems},
